{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "database"
      }
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/denoland-id/registry"
    }
  ],
  "rewrites": [
    {
      "source": "/schema.json",
      "destination": "https://raw.githubusercontent.com/denoland-id/registry/schema.json"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
